{
  "authenticationMethod": "none",
  "welcomeFile": "/webapp/index.html",
  "routes": [
    {
      "path": "/resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/resources"
      },
      "description": "SAPUI5 Resources"
    },
    {
      "path": "/test-resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/test-resources"
      },
      "description": "SAPUI5 Test Resources"
    },
    {
      "path": "/MobiOdata",
      "target": {
        "type": "destination",
        "name": "MobiOdata"
      },
      "description": "MobiOdata"
    },
     {
      "path": "/S4HanaOdata",
      "target": {
        "type": "destination",
        "name": "S4HanaOdata"
      },
      "description": "S4HanaOdata"
    },
    {
      "path": "/xsService",
      "target": {
        "type": "destination",
        "name": "xsService",
        "entryPath": "/"
      },
      "description": "sxservice"
    }
  ],
  "sendWelcomeFileRedirect": true,
  "cacheControl": [
    {
      "directive": "private",
      "maxAge": 0,
      "path": "**.html"
    },
    {
      "directive": "private",
      "maxAge": 0,
      "path": "sap-ui-cachebuster-info.json"
    },
    {
      "directive": "public",
      "maxAge": 0
    }
  ]
}